{
  "name": "battle-ship",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "./projects/*"
  ],
  "scripts": {
    "build:models:dev": "ng build models --configuration development",
    "build:models:prod": "ng build models --configuration production",
    "build:store-tools:dev": "ng build store-tools --configuration development",
    "build:store-tools:prod": "ng build store-tools --configuration production",
    "build:store:dev": "ng build store --configuration development",
    "build:store:prod": "ng build store --configuration production",
    "clean:all": "npm run clean:projects && npm run clean:root",
    "clean:projects": "lerna clean --yes",
    "clean:root": "rimraf node_module",
    "cz": "git-cz",
    "preinstall": "node ./scripts/check-yarn.js",
    "postinstall": "lerna bootstrap",
    "lint": "ng lint",
    "sort-package-json": "sort-package-json && lerna exec sort-package-json"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "@ngrx/effects": "^12.4.0",
    "@ngrx/entity": "^12.4.0",
    "@ngrx/store": "^12.4.0",
    "@ngrx/store-devtools": "^12.4.0",
    "ramda": "^0.27.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.7",
    "@angular-eslint/builder": "12.5.0",
    "@angular-eslint/eslint-plugin": "12.5.0",
    "@angular-eslint/eslint-plugin-template": "12.5.0",
    "@angular-eslint/schematics": "12.5.0",
    "@angular-eslint/template-parser": "12.5.0",
    "@angular/cli": "~12.2.7",
    "@angular/compiler-cli": "~12.2.0",
    "@commitlint/cli": "^13.2.0",
    "@commitlint/config-conventional": "^13.2.0",
    "@ngrx/schematics": "^12.4.0",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "@types/ramda": "^0.27.45",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.0",
    "immer": "^9.0.6",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "lerna": "^4.0.0",
    "ng-packagr": "^12.1.1",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "sort-package-json": "^1.52.0",
    "typescript": "~4.3.5"
  },
  "engines": {
    "node": ">= 14 "
  }
}
